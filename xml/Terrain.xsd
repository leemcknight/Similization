<?xml version="1.0" ?>
<xs:schema id="terrains" targetNamespace="http://tempuri.org/Terrain.xsd" xmlns:mstns="http://tempuri.org/Terrain.xsd" xmlns="http://tempuri.org/Terrain.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
	<xs:element name="terrains" msdata:IsDataSet="true" msdata:EnforceConstraints="False">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="terrain">
					<xs:complexType>
						<xs:attribute name="id" form="unqualified" type="xs:ID" />
						<xs:attribute name="name" form="unqualified" type="xs:string" />
						<xs:attribute name="food" form="unqualified" type="xs:int" />
						<xs:attribute name="shields" form="unqualified" type="xs:int" />
						<xs:attribute name="commerce" form="unqualified" type="xs:int" />
						<xs:attribute name="movecost" form="unqualified" type="xs:int" />
						<xs:attribute name="defense" form="unqualified" type="xs:int" />
						<xs:attribute name="irrigation_bonus" form="unqualified" type="xs:int" />
						<xs:attribute name="mining_bonus" form="unqualified" type="xs:int" />
						<xs:attribute name="road_bonus" form="unqualified" type="xs:int" />
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>