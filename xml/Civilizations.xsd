<?xml version="1.0" ?>
<xs:schema id="civilizations" targetNamespace="http://tempuri.org/Civilizations.xsd" xmlns:mstns="http://tempuri.org/Civilizations.xsd" xmlns="http://tempuri.org/Civilizations.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
	<xs:element name="civilizations" msdata:IsDataSet="true" msdata:EnforceConstraints="False">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="civilization">
					<xs:complexType>
						<xs:attribute name="id" form="unqualified" type="xs:ID" />
						<xs:attribute name="name" form="unqualified" type="xs:string" />
						<xs:attribute name="commerical" form="unqualified" type="xs:string" />
						<xs:attribute name="expansionist" form="unqualified" type="xs:string" />
						<xs:attribute name="industrious" form="unqualified" type="xs:string" />
						<xs:attribute name="militaristic" form="unqualified" type="xs:string" />
						<xs:attribute name="religious" form="unqualified" type="xs:string" />
						<xs:attribute name="scientific" form="unqualified" type="xs:string" />
						<xs:attribute name="globalculture" form="unqualified" type="xs:string" />
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>